<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/flag"
    android:orientation="vertical"
    tools:context="com.sample.com.sample2.AdminActivity"
    tools:showIn="@layout/app_bar_admin">

    <android.support.design.widget.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <android.support.v7.widget.Toolbar
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:title="Home"
            app:navigationIcon="@mipmap/ic_kunsh"
            />
    </android.support.design.widget.AppBarLayout>

    <FrameLayout
        android:id="@+id/content"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">

        <TextView
            android:layout_width="88dp"
            android:layout_height="40dp"
            android:layout_marginBottom="@dimen/activity_vertical_margin"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="16dp"
            android:layout_marginTop="25dp"
            android:text="UserID"
            android:textColor="@color/common_google_signin_btn_text_dark_focused"
            android:textSize="18sp"
            android:visibility="gone" />

        <EditText
            android:id="@+id/etxt"
            android:layout_width="126dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="105dp"
            android:layout_marginRight="16dp"
            android:layout_marginTop="20dp"
            android:visibility="gone" />

        <ListView
            android:id="@+id/user_list"
            android:layout_width="match_parent"
            android:layout_height="match_parent"

            android:visibility="gone" />

        <Button
            android:id="@+id/btn"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/activity_vertical_margin"
            android:layout_marginLeft="240dp"
            android:layout_marginRight="16dp"
            android:layout_marginTop="20dp"
            android:text="Search"
            android:textSize="14sp"
            android:visibility="gone" />

        <Spinner
            android:id="@+id/user_spin"
            android:layout_width="187dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/activity_vertical_margin"
            android:layout_marginLeft="160dp"
            android:layout_marginRight="50dp"
            android:layout_marginTop="10dp"
            android:visibility="gone" />

        <Spinner
            android:id="@+id/dt_spin"
            android:layout_width="189dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/activity_vertical_margin"
            android:layout_marginLeft="160dp"
            android:layout_marginRight="50dp"
            android:layout_marginTop="40dp"
            android:visibility="gone" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/activity_vertical_margin"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="50dp"
            android:layout_marginTop="15dp"
            android:text="Select UserID"
            android:textColor="@color/common_google_signin_btn_text_dark_focused"
            android:textSize="18sp"
            android:visibility="gone" />

        <TextView
            android:id="@+id/teiew8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/activity_vertical_margin"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="50dp"
            android:layout_marginTop="45dp"
            android:text="Select Date-Time"
            android:textColor="@color/common_google_signin_btn_text_dark_focused"
            android:textSize="18sp"
            android:visibility="gone" />

        <TextView
            android:id="@+id/n"
            android:layout_width="224dp"
            android:layout_height="32dp"
            android:layout_marginBottom="@dimen/activity_vertical_margin"
            android:layout_marginLeft="115dp"
            android:layout_marginRight="50dp"
            android:layout_marginTop="90dp"
            android:text="Updating..."
            android:textColor="@color/red_new"
            android:textSize="16sp"
            android:visibility="gone" />

        <TextView
            android:id="@+id/textView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/activity_vertical_margin"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="50dp"
            android:layout_marginTop="120dp"
            android:text="Gender"
            android:textColor="@color/common_google_signin_btn_text_dark_focused"
            android:textSize="16sp"
            android:textStyle="bold"
            android:visibility="gone" />

        <TextView
            android:id="@+id/textView15"
            android:layout_width="78dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/activity_vertical_margin"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="50dp"
            android:layout_marginTop="150dp"
            android:text="Latitude"
            android:textColor="@color/common_google_signin_btn_text_dark_focused"
